{
  "expo": {
    "name": "De Olho na Validade",
    "slug": "de-olho-na-validade",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.osmargyn.deolhonavalidade"
    },
    "web": {
      "favicon": "./src/assets/logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir que o App acesse a câmera para ler códigos de barras e tirar fotos dos produtos."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "seu-id-do-projeto-aqui"
      }
    }
  }
}